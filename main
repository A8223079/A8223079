#include <iostream>
#include<string>
#include"OOXX.h"
using namespace std;

void TicTacToe()//create first chart for game
{
    string list[9] = { "1","2","3" ,"4" ,"5" ,"6" ,"7" ,"8" ,"9" };
    int set = 0;
    for (int i = 0; i < 5; i++)
    {
        for (int j = 0; j < 5; j++)
        {
            if ((i == 0  i == 2  i == 4) && (j == 1  j == 3))
                cout << "|";

            if ((i == 1  i == 3) && (j == 0  j == 2  j == 4))
                cout << "-";

            if ((i == 1  i == 3) && (j == 1  j == 3))
                cout << "+";

            if ((i == 0  i == 2  i == 4) && (j == 0  j == 2  j == 4))
            {
                cout << list[set];
                set++;
            }
        }
        cout << endl;
    }
} 
